<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.desige.webDocuments.sacop.forms.PlantillaAccion" table="tbl_planillasacopaccion">
	
        	<id name="idplanillasacopaccion" type="java.lang.Long" column="idplanillasacopaccion">
			<generator class="assigned"/>
		</id>

                <property name="idplanillasacop1" type="long">
                          <column name="idplanillasacop1"/>
                </property>

                <property name="accion">
                          <column name="accion"/>
                </property>

               
                <property name="fecha" >
                          <column name="fecha"/>
                </property>

                <property name="responsables">
                          <column name="responsables"/>
                </property>
 
                <property name="active">
                          <column name="active"/>
                </property>
	</class>

      

	<query name="Planillasacopaccion.alls"><![CDATA[ FROM PlantillaAccion Order by fecha]]></query>
</hibernate-mapping>

